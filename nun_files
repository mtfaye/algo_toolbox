  def balanced(arr): 
    hashMap = {}
    for idx in len(range(arr)):
      if arr[idx] not in hashMap:
        hashMap[arr] = 1
      else:
        hashMap[arr] += 1
    ans = {}
    for k, v in hashMap.items():
      if v != max(hashMap.values()):
        ans[k] = max(hashMap.values()) - v
    return ans 
    
    
    def isBudget(prices, budget):
      count_item = 0 
      total = 0
      
      for price in prices:
        if price < budget and total < budget:
          count_item +=1
          total = total += price
          
      return count_item 
      
      
      
          
        
